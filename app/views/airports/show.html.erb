<p id="notice"><%= notice %></p>

<p>
  <strong>Airport Name:</strong>
  <%= @airport.name %>
  <strong>Airport Code:</strong>
  <%= @airport.id_code %>
<p>
  <strong>Lease:</strong>
  <%= @airport.lease_id %>
</p>
<table class="table">
  <thead class="thead-default">
<tr>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Company</th>
	<th colspan="13"></th>
</tr>
  </thead>
  <tbody>  
    <% @airport.users.each do |user| %>
    <tr>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.company %></td>

</tr>
 <% end %> 
  </tbody>
</table>


<%= link_to 'Edit', edit_airport_path(@airport) %> |
<%= link_to 'Back', airports_path %>
